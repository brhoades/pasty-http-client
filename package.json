{
  "name": "pasty-http-client",
  "version": "0.1.1",
  "description": "Anonymous, encrypted pastebin for files.",
  "scripts": {
    "upload": "yarn build && cp aws-upload.conf.js dist/ && cd dist && s3-upload && rm aws-upload.conf.js",
    "devserver": "cross-env NODE_ENV=development webpack-dev-server --hot --progress --cache --debug",
    "build": "cross-env NODE_ENV=production webpack -p --progress --hide-modules",
    "stats": "cross-env NODE_ENV=production webpack --profile --json > stats.json"
  },
  "dependencies": {
    "av-ts": "^0.7.1",
    "clipboard": "^1.6.1",
    "highlight.js": "^9.11.0",
    "pasty-core": "brhoades/pasty-core",
    "pgwcookie": "^1.4.0",
    "purecss": "^0.6.2",
    "spinkit": "^1.2.5",
    "vue": "^2.2.1",
    "vue-clip": "^1.0.0",
    "vue-router": "^2.2.1",
    "zepto-webpack": "^1.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-latest": "^6.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.28.0",
    "exports-loader": "^0.6.4",
    "html-webpack-plugin": "^2.29.0",
    "s3-upload": "^0.1.0",
    "style-loader": "^0.16.1",
    "ts-loader": "^2.0.3",
    "typescript": "^2.2.2",
    "vue-loader": "^12.0.3",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  }
